<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>QueryServlet</servlet-name>
        <servlet-class>ece358.QueryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonalInfo</servlet-name>
        <servlet-class>ece358.PersonalInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientLookup</servlet-name>
        <servlet-class>ece358.PatientLookup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prescriptions</servlet-name>
        <servlet-class>ece358.PrescriptionsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorLookupServlet</servlet-name>
        <servlet-class>ece358.DoctorLookupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorLookupServlet</servlet-name>
        <servlet-class>ece358.DoctorLookupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduledOperations</servlet-name>
        <servlet-class>ece358.ScheduledOperations</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryServlet</servlet-name>
        <url-pattern>/QueryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonalInfo</servlet-name>
        <url-pattern>/PersonalInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientLookup</servlet-name>
        <url-pattern>/PatientLookup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorLookupServlet</servlet-name>
        <url-pattern>/DoctorLookupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduledOperations</servlet-name>
        <url-pattern>/ScheduledOperations</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
